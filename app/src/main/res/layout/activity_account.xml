<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/includeTitle"
        layout="@layout/app_titl"/>

    <com.huaop2p.yqs.widget.ReboundScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="none"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/beijing_big"
            android:orientation="vertical">


            <include layout="@layout/include_linearlayout_left_space_view"></include>
            <RelativeLayout
                android:id="@+id/rl_ziliao"
                android:background="@color/white"
                style="@style/relative_item_list_layout_nospace">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <ImageView
                        style="@style/relative_item_list_left_img"
                        android:src="@drawable/mziliao"/>

                    <TextView
                        style="@style/relative_item_list_text"
                        android:text="@string/str_ziliao"/>
                </LinearLayout>

                <ImageView style="@style/relative_item_list_toright_img"/>
            </RelativeLayout>


            <include layout="@layout/include_linearlayout_left_space_view"></include>

            <RelativeLayout
                android:id="@+id/rl_shenfen"
                android:background="@color/white"
                style="@style/relative_item_list_layout_nospace">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <ImageView
                        style="@style/relative_item_list_left_img"
                        android:src="@drawable/renzheng"/>

                    <TextView
                        style="@style/relative_item_list_text"
                        android:text="@string/str_identity_valitor"/>
                </LinearLayout>


                <ImageView
                    android:id="@+id/img_jian"
                    style="@style/relative_item_list_toright_img"/>

                <TextView
                    android:id="@+id/txt_ok"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text=""
                    android:layout_marginLeft="250dp"
                    />
            </RelativeLayout>
            <include layout="@layout/include_linearlayout_left_space_view"></include>



            <RelativeLayout
                android:id="@+id/rl_ercode"
                android:background="@color/white"

                style="@style/relative_item_list_layout_nospace">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <ImageView
                        style="@style/relative_item_list_left_img"
                        android:src="@drawable/ercode"/>

                    <TextView
                        style="@style/relative_item_list_text"
                        android:text="@string/str_erweima"/>
                </LinearLayout>

                <ImageView style="@style/relative_item_list_toright_img"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"/>
            <include layout="@layout/include_linearlayout_left_space_view"></include>

            <RelativeLayout
                android:id="@+id/rl_xiugai"
                android:background="@color/white"
                style="@style/relative_item_list_layout_nospace">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <ImageView
                        style="@style/relative_item_list_left_img"
                        android:src="@drawable/xiugai"/>

                    <TextView
                        style="@style/relative_item_list_text"
                        android:text="@string/str_xiugai"/>
                </LinearLayout>

                <ImageView style="@style/relative_item_list_toright_img"/>
            </RelativeLayout>

            <include layout="@layout/include_linearlayout_left_space_view"></include>


            <RelativeLayout
                android:id="@+id/rl_kaiqi"
                android:background="@color/white"
                style="@style/relative_item_list_layout_nospace">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <ImageView
                        style="@style/relative_item_list_left_img"
                        android:src="@drawable/kaiqi" />

                    <TextView
                        style="@style/relative_item_list_text"
                        android:text="@string/str_open_gesture_pass" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="3dp"
                    android:gravity="center">


                    <com.huaop2p.yqs.widget.SlidSwitch
                        android:id="@+id/switchSetGesture"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:paddingRight="5dp" />
                </LinearLayout>

            </RelativeLayout>


            <include layout="@layout/include_linearlayout_left_space_view"></include>

            <RelativeLayout
                android:id="@+id/rl_shezhiss"
                android:background="@color/white"

                style="@style/relative_item_list_layout_nospace">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <ImageView
                        style="@style/relative_item_list_left_img"
                        android:src="@drawable/shoushi"/>

                    <TextView
                        android:id="@+id/txt_shoushimm"
                        style="@style/relative_item_list_text"
                        android:text="@string/str_shezhishoushi"/>
                </LinearLayout>

                <ImageView style="@style/relative_item_list_toright_img"/>
            </RelativeLayout>

            <include layout="@layout/include_linearlayout_left_space_view"></include>
            <Button
                android:layout_width="match_parent"
                android:layout_height="350dp"
                android:background="@color/transparent"
                />


        </LinearLayout>


    </com.huaop2p.yqs.widget.ReboundScrollView>

</LinearLayout>